# 342 keV branch
/gps/energy 342 keV
/control/multiply nEventsUno {decayedNucleik} 67
/run/beamOn {nEventsUno}
/analysis/setFileName realThreeCylindersDiameter{cylDiam}mmDistance{distanceZ}mmP3Primo342keV.root

/control/divide v1 1.24 6.7
/control/remainder v2 {nEventsUno} {v1}
/control/subtract v3 {nEventsUno} {v2}
/control/divide nEventsDue {v3} {v1}

# 245 keV branch
/gps/energy 245 keV
/control/multiply nEventsDue {decayedNucleik} 12
/run/beamOn {nEventsDue}
/analysis/setFileName realThreeCylindersDiameter{cylDiam}mmDistance{distanceZ}mmP3Primo245keV.root
